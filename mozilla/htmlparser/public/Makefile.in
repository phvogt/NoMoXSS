DEPTH       = ../..
topsrcdir   = @top_srcdir@
srcdir      = @srcdir@
VPATH       = @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE      = htmlparser
GRE_MODULE	= 1

XPIDLSRCS = \
	nsIExpatSink.idl \
	$(NULL)

EXPORTS = \
		nsIContentSink.h            \
		nsITokenizer.h              \
		nsIHTMLContentSink.h        \
		nsIHTMLFragmentContentSink.h\
		nsIParserNode.h             \
		nsIParser.h                 \
		nsIDTD.h                    \
		nsIParserFilter.h           \
		nsIElementObserver.h        \
		nsIParserService.h          \
		nsHTMLTagList.h             \
		nsHTMLTags.h                \
		nsHTMLTokens.h              \
		nsScannerString.h           \
		nsParserError.h             \
		nsParserCIID.h              \
		nsToken.h                   \
		$(NULL)

ifdef MOZ_DEBUG
EXPORTS += \
		nsILoggingSink.h \
		$(NULL)
endif

EXPORTS   := $(addprefix $(srcdir)/, $(EXPORTS))

include $(topsrcdir)/config/rules.mk
