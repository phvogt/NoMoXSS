DEPTH		= ../../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= uninstall
PROGRAM		= uninstall$(BIN_SUFFIX)
RESFILE		= uninstall.res
REQUIRES	= $(ZLIB_REQUIRES)
EXTRA_DSO_LIBS	= mozz_s
USE_NON_MT_LIBS = 1
USE_STATIC_LIBS = 1

EXPORTS		= logkeys.h

CSRCS		= \
		uninstall.c \
		extra.c \
		dialogs.c \
		parser.c \
		ifuncns.c \
		rdi.c \
		$(NULL)

CPPSRCS		= \
		nsINIParser.cpp \
		$(NULL)


NO_DIST_INSTALL	= 1

include $(topsrcdir)/config/rules.mk

LDFLAGS += -Zlinker /PM:PM

libs:: $(PROGRAM)
	$(INSTALL) $(PROGRAM) $(DIST)/install

install:: $(PROGRAM)
	$(INSTALL) $(PROGRAM) $(DESTDIR)$(mozappdir)/install

clean clobber realclean clobber_all::
	$(RM) $(DIST)/install/$(PROGRAM)

