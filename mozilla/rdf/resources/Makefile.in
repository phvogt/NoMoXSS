DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

FILES		= \
		folder-closed.gif \
		folder-open.gif \
		loading.gif \
		dom-test-4.css \
		ignore-test.xul \
		$(NULL)

include $(topsrcdir)/config/rules.mk

libs::
	$(INSTALL) $(addprefix $(srcdir)/, $(FILES)) $(DIST)/bin/res/rdf

install:: $(FILES)
	$(SYSINSTALL) $(IFLAGS1) $^ $(DESTDIR)$(mozappdir)/res/rdf

