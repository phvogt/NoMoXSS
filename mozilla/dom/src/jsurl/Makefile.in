DEPTH		= ../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= jsurl
LIBRARY_NAME	= jsurl_s
FORCE_STATIC_LIB = 1
MODULE_NAME	= javascript__protocol
GRE_MODULE	= 1

REQUIRES	= xpcom \
		  string \
		  js \
		  dom \
		  necko \
		  caps \
		  widget \
		  appshell \
		  xpconnect \
		  jsconsole \
		  uconv \
		  docshell \
		  $(NULL)

XPIDLSRCS	= \
		nsIEvaluateStringProxy.idl \
		$(NULL)


CPPSRCS		= nsJSProtocolHandler.cpp

EXPORTS		= $(srcdir)/nsJSProtocolHandler.h

EXTRA_DSO_LDOPTS = \
		$(MOZ_COMPONENT_LIBS) \
		$(MOZ_JS_LIBS) \
		$(NULL)

ifdef BUILD_DEBUG_GC
DEFINES		+= -DDEBUG_GC
endif

include $(topsrcdir)/config/rules.mk

