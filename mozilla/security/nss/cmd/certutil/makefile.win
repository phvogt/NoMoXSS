include <$(DEPTH)\config\rules.mak>


INSTALL	= $(MAKE_INSTALL)

objs:	$(OBJS)

$(PROGRAM)::
	$(INSTALL) $(DIST)/bin/pr3240.dll ./$(OBJDIR)

programs: $(PROGRAM)

install:: $(TARGETS)
	$(INSTALL) $(TARGETS) $(DIST)/bin

