NODEPEND=1
IGNORE_MANIFEST = 1

MODULE = urltest

DEPTH= ..\..\..
REQUIRES	= xpcom \
                  necko \
                  string

MAKE_OBJ_TYPE=EXE

EXENAME=urltest
PDBFILE=nunettest.pdb
MAPFILE=nunettest.map

OBJS=                                   \
        .\$(OBJDIR)\urltest.obj         \
        $(NULL)

PROGRAM=.\$(OBJDIR)\$(EXENAME).EXE

LCFLAGS=-DNETSCAPE
LLIBS=$(LLIBS)				\
		$(LIBNSPR)              \
		$(DIST)\lib\xpcom.lib	\
		$(DIST)\lib\nunet.lib  \
		$(DIST)\lib\netlib.lib

# clobber and clobber_all will remove the following garbage:
GARBAGE	= $(GARBAGE) _gen

include <$(DEPTH)/config/rules.mak>

libs:: $(PROGRAM)
    $(MAKE_INSTALL) $(PROGRAM) $(DIST)\bin

clobber::
	rm -f $(DIST)\bin\$(EXENAME).exe
