. $topsrcdir/browser/config/mozconfig
# CC=gcc
# CXX=g++
# CPP=cpp
# AS=as
# LD=ld
mk_add_options MOZ_OBJDIR=$topsrcdir/firefox_obj_dir
ac_add_options --disable-accessibility
